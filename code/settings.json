{
  "security.workspace.trust.untrustedFiles": "open",
  "files.trimTrailingWhitespace": true,
  "workbench.editor.closeEmptyGroups": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.iconTheme": "vscode-icons-mac",
  "workbench.list.smoothScrolling": true,
  "workbench.productIconTheme": "el-vsc-v1-icons",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "files.exclude": {
    ".venv**": true,
    "**/__pycache__": true,
    "**/.classpath": true,
    "**/.DS_Store": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.svn": true,
    "**/.venv": true,
    "**/*.bundle.js": true,
    "**/*.pyc": true,
    "**/**~": true,
    "**/CVS": true,
    "built/**": true,
    "core/public/**": true,
    "core/src/libs/**": true,
    "node_modules": true,
    "shared/vendor/**": true
  },
  "search.exclude": {
    "node_modules/**": true
  },
  "launch": {
    "configurations": [],
    "compounds": []
  },
  "vsicons.dontShowNewVersionMessage": true,
  "diffEditor.renderSideBySide": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorStyle": "block",
  "editor.fontWeight": "bold",
  "editor.formatOnPaste": true,
  "editor.hover.delay": 200,
  "editor.lineNumbers": "relative",
  "editor.mouseWheelScrollSensitivity": 1,
  "editor.renderLineHighlight": "gutter",
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 4,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "editor.rulers": [101],
  "editor.tokenColorCustomizations": {
    "[monokai-charcoal]": {
      "comments": "#63afff"
    }
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoFormatStrings": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.commandlineinprogress": "#007acc",
  "vim.statusBarColors.insert": "#5f0000",
  "vim.statusBarColors.normal": "#242424",
  "vim.statusBarColors.replace": "#00000",
  "vim.statusBarColors.visual": "#5f00af",
  "vim.statusBarColors.visualblock": "#86592d",
  "vim.statusBarColors.visualline": "#005f5f",
  "vim.vimrc.enable": false,
  "eslint.format.enable": true,
  "eslint.nodePath": "",
  "eslint.trace.server": "off",
  "eslint.options": {
    "overrideConfigFile": "/repo/.eslintrc.yml",
    "ignorePath": "/repo/.eslintignore"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnPaste": false,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit",
      "source.fixAll": "explicit"
    },
    "diffEditor.ignoreTrimWhitespace": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.run": "onSave",
  "errorLens.delay": 2000,
  "intellicodeApiExamples.python.enabled": false,
  "search.smartCase": true,
  "git.openRepositoryInParentFolders": "never",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "isort.args": ["--profile black"],
  "isort.check": true,
  "isort.importStrategy": "fromEnvironment",
  "eslint.codeActionsOnSave.rules": null,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f",
    "statusBar.foreground": "#ffffff",
    "statusBar.debuggingForeground": "#ffffff"
  },
  "editor.fontLigatures": true,
  "editor.codeLensFontFamily": "'SFMono-Regular', Menlo, Monaco, 'Courier New', monospace",
  "editor.inlayHints.fontFamily": "'SFMono-Regular', Menlo, Monaco, 'Courier New', monospace",
  "workbench.fontAliasing": "antialiased",
  "markdown.preview.fontFamily": "'SFMono-Regular', Menlo, Monaco, 'Courier New', monospace",
  "errorLens.fontFamily": "'SFMono-Regular', Menlo, Monaco, 'Courier New', monospace",
  "errorLens.fontStyleItalic": true,
  "editor.lineHeight": 1.45,
  "workbench.activityBar.location": "top",
  "notebook.output.textLineLimit": 200,
  "notebook.output.scrolling": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.tabSizingFixedMaxWidth": 40,
  "workbench.editor.wrapTabs": true,
  "editor.minimap.enabled": false,
  "editor.letterSpacing": 0.001,
  "window.commandCenter": false,
  "gitlens.ai.experimental.provider": "openai",
  "gitlens.proxy": {},
  "github.copilot.editor.enableAutoCompletions": true,
  "editor.fontFamily": "'SFMono-Regular', Menlo, Monaco, 'Courier New', monospace",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.renderWhitespace": "all",
  "jupyter.askForKernelRestart": false,
  "editor.stickyScroll.defaultModel": "foldingProviderModel",
  "chat.editing.alwaysSaveWithGeneratedChanges": true,
  "window.newWindowProfile": "Default",
  "workbench.colorTheme": "One Hunter Theme",
  "editor.formatOnSave": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "python.analysis.typeEvaluation.analyzeUnannotatedFunctions": true,
  "window.titleBarStyle": "custom"
}
